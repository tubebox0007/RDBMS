 CREATE TABLE emp(
 empid VARCHAR2(10) PRIMARY KEY,
 empnameVARCHAR2(20),
 dob DATE,
 dept VARCHAR2(15),
 salary NUMBER(10,2)
 );
 INSERT INTO emp VALUES ('1', 'HARRIE', '06-JUN-2002',
 'IT', 45000);
INSERT INTO emp
 VALUES(:EMPID, :EMPNAME, :DOB, :DEPT, :SALARY);
 SELECT * FROM emp;
 SELECT empid, empname, salary FROM emp;
 SELECT * FROM empWHEREsalary>=30000;
 SELECT * FROM empWHEREempnameLIKE'H%';
 SELECT * FROM empWHEREdept='IT'ORDER BYsalary;
 SELECT * FROM empORDERBYsalary DESC;
 UPDATE empSETdept='ACCOUNTS' WHERE empid=1;
 UPDATE empSETsalary = salary + 500;
 UPDATE empSETdept='ACCOUNTS', salary = 33000
 WHEREempid=3;
 DELETE FROM empWHEREempid=6;
 DELETE FROM emp;
